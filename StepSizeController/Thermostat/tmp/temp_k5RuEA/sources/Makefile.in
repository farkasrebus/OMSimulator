# Makefile generated by OpenModelica
CC=@CC@
AR=@AR@
CFLAGS=@CFLAGS@
LD=$(CC) -shared
LDFLAGS=@LDFLAGS@ @LIBS@
DLLEXT=@DLLEXT@
NEED_RUNTIME=@NEED_RUNTIME@
NEED_DGESV=@NEED_DGESV@
FMIPLATFORM=@FMIPLATFORM@
# Note: Simulation of the fmu with dymola does not work with -finline-small-functions (enabled by most optimization levels)
CPPFLAGS=@CPPFLAGS@
OMC_NUM_LINEAR_SYSTEMS=0
OMC_NUM_NONLINEAR_SYSTEMS=0
OMC_NUM_MIXED_SYSTEMS=0
OMC_NDELAY_EXPRESSIONS=0
OMC_NVAR_STRING=0

override CPPFLAGS += -Iinclude/ -Iinclude/fmi2 -I.    -DOMC_MODEL_PREFIX=ThermostatMonitor_CentralMonitor -DOMC_NUM_MIXED_SYSTEMS=0 -DOMC_NUM_LINEAR_SYSTEMS=0 -DOMC_NUM_NONLINEAR_SYSTEMS=0 -DOMC_NDELAY_EXPRESSIONS=0 -DOMC_NVAR_STRING=0

MAINFILE=ThermostatMonitor_CentralMonitor_FMU.c
MAINOBJ=ThermostatMonitor_CentralMonitor_FMU.o
CFILES=ThermostatMonitor_CentralMonitor.c ThermostatMonitor_CentralMonitor_functions.c ThermostatMonitor_CentralMonitor_records.c \
ThermostatMonitor_CentralMonitor_01exo.c ThermostatMonitor_CentralMonitor_02nls.c ThermostatMonitor_CentralMonitor_03lsy.c ThermostatMonitor_CentralMonitor_04set.c ThermostatMonitor_CentralMonitor_05evt.c ThermostatMonitor_CentralMonitor_06inz.c ThermostatMonitor_CentralMonitor_07dly.c \
ThermostatMonitor_CentralMonitor_08bnd.c ThermostatMonitor_CentralMonitor_09alg.c ThermostatMonitor_CentralMonitor_10asr.c ThermostatMonitor_CentralMonitor_11mix.c ThermostatMonitor_CentralMonitor_12jac.c ThermostatMonitor_CentralMonitor_13opt.c ThermostatMonitor_CentralMonitor_14lnz.c \
ThermostatMonitor_CentralMonitor_15syn.c ThermostatMonitor_CentralMonitor_16dae.c ThermostatMonitor_CentralMonitor_17inl.c ThermostatMonitor_CentralMonitor_init_fmu.c
OFILES=$(CFILES:.c=.o)
GENERATEDFILES=$(MAINFILE) ThermostatMonitor_CentralMonitor_FMU.makefile ThermostatMonitor_CentralMonitor_literals.h ThermostatMonitor_CentralMonitor_model.h ThermostatMonitor_CentralMonitor_includes.h ThermostatMonitor_CentralMonitor_functions.h  ThermostatMonitor_CentralMonitor_11mix.h ThermostatMonitor_CentralMonitor_12jac.h ThermostatMonitor_CentralMonitor_13opt.h ThermostatMonitor_CentralMonitor_init_fmu.c ThermostatMonitor_CentralMonitor_info.c $(CFILES) ThermostatMonitor_CentralMonitor_FMU.libs

# FIXME: before you push into master...
RUNTIMEDIR=include
OMC_MINIMAL_RUNTIME=1
OMC_FMI_RUNTIME=1
include $(RUNTIMEDIR)/Makefile.objs
ifneq ($(NEED_RUNTIME),)
RUNTIMEFILES=$(FMI_ME_OBJS:%=$(RUNTIMEDIR)/%.o)
endif

PHONY: ThermostatMonitor_CentralMonitor_FMU
ThermostatMonitor_CentralMonitor_FMU: $(MAINOBJ) ThermostatMonitor_CentralMonitor_functions.h ThermostatMonitor_CentralMonitor_literals.h $(OFILES) $(RUNTIMEFILES)
	mkdir -p ../binaries/$(FMIPLATFORM)
ifeq (@LIBTYPE_DYNAMIC@,1)
	$(LD) -o ThermostatMonitor_CentralMonitor$(DLLEXT) $(MAINOBJ) $(OFILES) $(RUNTIMEFILES) -L"/home/farkasr/git/OMSimulator/StepSizeController/Thermostat"   $(LDFLAGS)
	cp ThermostatMonitor_CentralMonitor$(DLLEXT) ThermostatMonitor_CentralMonitor_FMU.libs config.log ../binaries/$(FMIPLATFORM)/
endif
ifeq (@LIBTYPE_STATIC@,1)
	rm -f ThermostatMonitor_CentralMonitor.a
	$(AR) -rsu ThermostatMonitor_CentralMonitor.a $(MAINOBJ) $(OFILES) $(RUNTIMEFILES)
	cp ThermostatMonitor_CentralMonitor.a ThermostatMonitor_CentralMonitor_FMU.libs config.log ../binaries/$(FMIPLATFORM)/
endif
	$(MAKE) distclean
	cd .. && rm -f ../ThermostatMonitor_CentralMonitor.fmu && zip -r ../ThermostatMonitor_CentralMonitor.fmu *
distclean: clean
	rm -f Makefile config.status config.log
clean:
	rm -f ThermostatMonitor_CentralMonitor.def ThermostatMonitor_CentralMonitor.o ThermostatMonitor_CentralMonitor.a ThermostatMonitor_CentralMonitor$(DLLEXT) $(MAINOBJ) $(OFILES) $(RUNTIMEFILES)