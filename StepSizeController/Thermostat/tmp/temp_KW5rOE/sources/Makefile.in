# Makefile generated by OpenModelica
CC=@CC@
AR=@AR@
CFLAGS=@CFLAGS@
LD=$(CC) -shared
LDFLAGS=@LDFLAGS@ @LIBS@
DLLEXT=@DLLEXT@
NEED_RUNTIME=@NEED_RUNTIME@
NEED_DGESV=@NEED_DGESV@
FMIPLATFORM=@FMIPLATFORM@
# Note: Simulation of the fmu with dymola does not work with -finline-small-functions (enabled by most optimization levels)
CPPFLAGS=@CPPFLAGS@
OMC_NUM_LINEAR_SYSTEMS=2
OMC_NUM_NONLINEAR_SYSTEMS=0
OMC_NUM_MIXED_SYSTEMS=0
OMC_NDELAY_EXPRESSIONS=0
OMC_NVAR_STRING=0

override CPPFLAGS += -Iinclude/ -Iinclude/fmi2 -I.    -DOMC_MODEL_PREFIX=Room -DOMC_NUM_MIXED_SYSTEMS=0 -DOMC_NUM_LINEAR_SYSTEMS=2 -DOMC_NUM_NONLINEAR_SYSTEMS=0 -DOMC_NDELAY_EXPRESSIONS=0 -DOMC_NVAR_STRING=0

MAINFILE=Room_FMU.c
MAINOBJ=Room_FMU.o
CFILES=Room.c Room_functions.c Room_records.c \
Room_01exo.c Room_02nls.c Room_03lsy.c Room_04set.c Room_05evt.c Room_06inz.c Room_07dly.c \
Room_08bnd.c Room_09alg.c Room_10asr.c Room_11mix.c Room_12jac.c Room_13opt.c Room_14lnz.c \
Room_15syn.c Room_16dae.c Room_17inl.c Room_init_fmu.c
OFILES=$(CFILES:.c=.o)
GENERATEDFILES=$(MAINFILE) Room_FMU.makefile Room_literals.h Room_model.h Room_includes.h Room_functions.h  Room_11mix.h Room_12jac.h Room_13opt.h Room_init_fmu.c Room_info.c $(CFILES) Room_FMU.libs

# FIXME: before you push into master...
RUNTIMEDIR=include
OMC_MINIMAL_RUNTIME=1
OMC_FMI_RUNTIME=1
include $(RUNTIMEDIR)/Makefile.objs
ifneq ($(NEED_RUNTIME),)
RUNTIMEFILES=$(FMI_ME_OBJS:%=$(RUNTIMEDIR)/%.o)
endif

PHONY: Room_FMU
Room_FMU: $(MAINOBJ) Room_functions.h Room_literals.h $(OFILES) $(RUNTIMEFILES)
	mkdir -p ../binaries/$(FMIPLATFORM)
ifeq (@LIBTYPE_DYNAMIC@,1)
	$(LD) -o Room$(DLLEXT) $(MAINOBJ) $(OFILES) $(RUNTIMEFILES) -L"/media/m_krisztian/Data/M_Krisztian/git/opencps-hungary/models/modelica/room_controller"   $(LDFLAGS)
	cp Room$(DLLEXT) Room_FMU.libs config.log ../binaries/$(FMIPLATFORM)/
endif
ifeq (@LIBTYPE_STATIC@,1)
	rm -f Room.a
	$(AR) -rsu Room.a $(MAINOBJ) $(OFILES) $(RUNTIMEFILES)
	cp Room.a Room_FMU.libs config.log ../binaries/$(FMIPLATFORM)/
endif
	$(MAKE) distclean
	cd .. && rm -f ../Room.fmu && zip -r ../Room.fmu *
distclean: clean
	rm -f Makefile config.status config.log
clean:
	rm -f Room.def Room.o Room.a Room$(DLLEXT) $(MAINOBJ) $(OFILES) $(RUNTIMEFILES)